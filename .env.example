#!/bin/sh
# ~/.env.example
# Copy this to ~/.env and fill in your actual tokens
# Add ~/.env to your .gitignore!

# Mapbox token for Rerun visualization
export MAPBOX_ACCESS_TOKEN="your_mapbox_token_here"
export RERUN_MAPBOX_ACCESS_TOKEN="$MAPBOX_ACCESS_TOKEN"

# GitHub Personal Access Token (for container registry, etc.)
export CR_PAT="your_github_pat_here"
